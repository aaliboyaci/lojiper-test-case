{
  "extends": "next/core-web-vitals",
  "settings": {
    "react": {
      "version": "detect"
    },
    "import/resolver": {
      "node": {
        "paths": ["app"],
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  },
  "rules": {
    "no-unused-vars": 1,
    "react/react-in-jsx-scope": "off"
  }
}
